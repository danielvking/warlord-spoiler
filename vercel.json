{
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public,max-age=31536000,s-maxage=31536000,immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public,max-age=31536000,s-maxage=31536000,immutable"
        }
      ]
    },
    {
      "source": "/resources/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public,max-age=604800,s-maxage=31536000"
        }
      ]
    }
  ]
}